{
  "domain": "ecommerce",
  "site": "alza.cz",
  "flow": "add_to_cart",
  "description": "Alza add to cart včetně cookie consent",
  "steps": [
    {
      "action": "goto",
      "url": "https://www.alza.cz"
    },
    {
      "action": "expectVisible",
      "selector": "#alzacookies"
    },
    {
      "action": "click",
      "selector": "#alzacookies a:has-text(\"Rozumím\")",
      "force": true
    },
    {
      "action": "expectNotVisible",
      "selector": "#alzacookies"
    },
    {
      "action": "expectVisible",
      "selector": "input[name='SearchText']"
    },
    {
      "action": "fill",
      "selector": "input[name='SearchText']",
      "value": "notebook"
    },
    {
      "action": "click",
      "selector": "button[type='submit']"
    }
  ]
}
